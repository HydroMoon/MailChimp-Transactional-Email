{
    "name": "applabqatar/mailchimp-transactional-email",
    "description": "This is my package mailchimp-transactional-email",
    "keywords": [
        "ApplabQatar",
        "laravel",
        "mailchimp-transactional-email"
    ],
    "homepage": "https://github.com/applabqatar/mailchimp-transactional-email",
    "license": "MIT",
    "authors": [
        {
            "name": "Qasim",
            "email": "vs.dev50@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "suggest": {
        "mailchimp/transactional": "For working with MailChimp"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mailchimp/transactional": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ApplabQatar\\MailChimpTransactionalEmail\\": "src/",
            "ApplabQatar\\MailChimpTransactionalEmail\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplabQatar\\MailChimpTransactionalEmail\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApplabQatar\\MailChimpTransactionalEmail\\MailChimpTransactionalEmailServiceProvider"
            ],
            "aliases": {
                "MailChimpTransactionalEmail": "ApplabQatar\\MailChimpTransactionalEmail\\Facades\\MailChimpTransactionalEmail"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
